{
  "name": "typescript-star-platinum",
  "version": "2.2.1",
  "description": "ストーン・フリー!",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm",
    "test": "jest",
    "lint": "eslint --ext .ts src/**/**.ts",
    "format": "prettier --write --parser typescript src/**/*ts",
    "postversion": "cp -r package.json .."
